<Window x:Class="WpfAppKino0410.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppKino0410"
        mc:Ignorable="d"
        Title="AuthWindow" Height="350" Width="400" ResizeMode="NoResize">
    <Grid>
        <StackPanel>
            <StackPanel Orientation="Horizontal" Height="40" VerticalAlignment="Top" HorizontalAlignment="Center" >
                <Button x:Name="GoAuthButton" Click="GoAuthButton_Click" Content="Авторизация" Width="120" Margin="5"/>
                <Button x:Name="GoRegButton" Click="GoRegButton_Click" Content="Регистрация" Width="120" Margin="5"/>
            </StackPanel>
            <Frame x:Name="Start" NavigationUIVisibility="Hidden" Width="300" Height="250" VerticalAlignment="Top" />
        </StackPanel>

    </Grid>
</Window>
